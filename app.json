{
  "expo": {
    "name": "ADV104_Pro",
    "slug": "ADV104_Pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo1.png",

    "scheme": "adv104pro",

    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["android"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFADD",
        "foregroundImage": "./assets/images/Logo1.png",
        "backgroundImage": "./assets/images/Logo1.png",
        "monochromeImage": "./assets/images/Logo1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kaizerklent.ADV104_Pro"
    },

    "web": {
      "output": "single",
      "favicon": "./assets/images/Logo.png"
    },

    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo1.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#FFFADD"
        }
      ],
      "expo-web-browser"
    ],

    "experiments": {
      "router": false,
      "typedRoutes": false,
      "reactCompiler": true
    },

    "extra": {
      "eas": {
        "projectId": "da0a0159-c19c-4fdf-9cf5-565ef1922d85"
      }
    }
  }
}
